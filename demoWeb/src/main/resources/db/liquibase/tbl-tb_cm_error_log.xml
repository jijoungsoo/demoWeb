<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:pro="http://www.liquibase.org/xml/ns/pro" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.1.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">
    <changeSet author="auto (g)" id="tb_cm_error_log-1">
                
        <createTable tableName="tb_cm_error_log" remarks="에러로그" >
                        
            <column name="error_log_id" type="BIGINT" remarks="에러 증가 id" >                                
                <constraints nullable="false" primaryKey="true" primaryKeyName="tb_cm_error_log_pkey"/>                            
            </column>
                        
            <column name="browser_kind" type="VARCHAR(500)" remarks="브라우저 종류"  />                        
            <column name="error_class" type="VARCHAR(200)"  remarks="에러 클래스" />
            <column name="error_msg" type="VARCHAR(2000)"  remarks="에러 메시지" />
            <column name="header_info" type="VARCHAR(4000)"  remarks="해더 정보" />
            <column name="log_date" type="TIMESTAMP WITHOUT TIME ZONE"  remarks="기록 시간" />
            <column name="param_info" type="VARCHAR(4000)"   remarks="파라미터 정보" />
            <column name="request_path" type="VARCHAR(2000)"   remarks="url" />
            <column name="server_ip" type="VARCHAR(50)"   remarks="서버 IP" />
            <column name="stack_trace" type="LONGTEXT"   remarks="에러 상세" />
            <column name="user_no" type="BIGINT"   remarks="유저 키" />
            <column name="client_ip" type="VARCHAR(50)" remarks="클라이언트 IP" />
        </createTable>
            
    </changeSet>
</databaseChangeLog>
