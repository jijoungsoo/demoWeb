            </column>
                                                
            
            
            <column name="job_name" type="VARCHAR(200)">
                                                                
                
                
                <constraints nullable="false" primaryKey="true" primaryKeyName="qrtz_job_details_pkey"/>
                                                            
            
            
            </column>
                                                
            
            
            <column name="job_group" type="VARCHAR(200)">
                                                                
                
                
