            <column name="is_nonconcurrent" type="BOOLEAN">
                                                                
                
                
                <constraints nullable="false"/>
                                                            
            
            
            </column>
                                                
            
            
            <column name="is_update_data" type="BOOLEAN">
                                                                
                
                
                <constraints nullable="false"/>
                                                            
            
            
            </column>
                                                
            
            
            <column name="requests_recovery" type="BOOLEAN">
                                                                
                
                
                <constraints nullable="false"/>
                                                            
            
            
            </column>
                                                
            
            
            <column name="job_data" type="BYTEA"/>
                                            
        
        
        </createTable>
                            
    
    
    </changeSet>
        

</databaseChangeLog>
