<?xml version="1.1" encoding="UTF-8"?><databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:pro="http://www.liquibase.org/xml/ns/pro" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.1.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">
    <changeSet author="auto (g)" id="tb_cm_job-4">
        <dropColumn  catalogName="public"  
            schemaName="public"  
            tableName="tb_cm_job">  
            <column  name="creation_date"/>  
            <column name="last_update_date" />
            <column name="creation_user_no"/>
            <column name="update_user_no"/>
            <column name="developer_name"/>
            <column name="job_name"/>
            

        </dropColumn>  

        <addColumn catalogName="public"  
			schemaName="public"
			tableName="tb_cm_job" >
            <column name="crt_dtm" type="TIMESTAMP WITHOUT TIME ZONE"  remarks="생성일" />
            <column name="updt_dtm" type="TIMESTAMP WITHOUT TIME ZONE"  remarks="수정일" />
            <column name="crt_user_no" type="BIGINT"  remarks="생성자NO" />
            <column name="updt_user_no" type="BIGINT"  remarks="수정자NO" />
            <column name="developer_nm" remarks="개발자" type="VARCHAR(100)" />
            <column name="job_nm" type="VARCHAR(200)"  remarks="JOB명" />

            

        </addColumn>  
    </changeSet>
    <changeSet author="auto (g)" id="tb_cm_job-3">
        <dropColumn  catalogName="public"  
            schemaName="public"  
            tableName="tb_cm_job">  
            <column  name="job_id"/>  
            <column name="creation_user_id" />
            <column name="last_update_user_id" />
        </dropColumn>  

        <addColumn catalogName="public"  
			schemaName="public"
			tableName="tb_cm_job" >
            <column name="job_seq" type="BIGINT"  remarks="JOB키">
                <constraints nullable="false" primaryKey="true" primaryKeyName="tb_cm_job_pkey"/>
            </column>
            <column name="creation_user_no" type="BIGINT"  remarks="생성자NO" />
            <column name="update_user_no" type="BIGINT"  remarks="수정자NO" />
        </addColumn>  
    </changeSet>

    <changeSet author="auto (g)" id="tb_cm_job-2">
        <dropColumn  catalogName="public"  
            schemaName="public"  
            tableName="tb_cm_job">  
            <column  name="job_task_code"/>  
            <column  name="job_type_code"/>  
            <column  name="job_param"/>  
        </dropColumn>  

        <addColumn catalogName="public"  
			schemaName="public"
			tableName="tb_cm_job" >
            <column name="br_nm" 
                type="VARCHAR(500)"
                remarks="BR명"
                />  
        </addColumn>  

        <setColumnRemarks catalogName="public"  columnName="job_id"  remarks="Job구분키" schemaName="public" tableName="tb_cm_job" /> 
        <setColumnRemarks catalogName="public"  columnName="job_name"  remarks="Job Name" schemaName="public" tableName="tb_cm_job" /> 
        <setColumnRemarks catalogName="public"  columnName="description"  remarks="Job설명" schemaName="public" tableName="tb_cm_job" /> 
        <setColumnRemarks catalogName="public"  columnName="developer_name"  remarks="개발자" schemaName="public" tableName="tb_cm_job" /> 
        <setColumnRemarks catalogName="public"  columnName="sort_order_no"  remarks="순번" schemaName="public" tableName="tb_cm_job" /> 
        <setColumnRemarks catalogName="public"  columnName="job_exec_time"  remarks="Job실행주기" schemaName="public" tableName="tb_cm_job" /> 
        <setColumnRemarks catalogName="public"  columnName="use_yn"  remarks="사용여부" schemaName="public" tableName="tb_cm_job" /> 
    </changeSet>

    <changeSet author="auto (g)" id="tb_cm_job-1">
        <createTable tableName="tb_cm_job">
            <column name="job_id" type="VARCHAR(36)">
                <constraints nullable="false" primaryKey="true" primaryKeyName="tb_cm_job_pkey"/>
            </column>
            <column name="creation_date" type="TIMESTAMP WITHOUT TIME ZONE"/>
            <column name="creation_user_id" type="VARCHAR(50)"/>
            <column name="last_update_user_id" type="VARCHAR(50)"/>
            <column name="last_update_date" type="TIMESTAMP WITHOUT TIME ZONE"/>
            <column name="description" type="VARCHAR(1000)"/>
            <column name="developer_name" type="VARCHAR(100)"/>
            <column name="job_exec_time" type="VARCHAR(500)"/>
            <column name="job_name" type="VARCHAR(200)"/>
            <column name="job_param" type="VARCHAR(100)"/>
            <column name="job_task_code" type="VARCHAR(50)"/>
            <column name="job_type_code" type="VARCHAR(50)"/>
            <column name="sort_order_no" type="INTEGER"/>
            <column name="use_yn" type="VARCHAR(1)"/>
        </createTable>
    </changeSet>
</databaseChangeLog>
